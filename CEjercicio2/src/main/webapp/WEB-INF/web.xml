<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerAjax</servlet-name>
  	<display-name>ControllerAjax</display-name>
  	<description></description>
  	<servlet-class>com.mauricio.controladores.ControllerAjax</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControllerFiles</servlet-name>
  	<display-name>ControllerFiles</display-name>
  	<description></description>
  	<servlet-class>com.mauricio.controladores.ControllerFiles</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerAjax</servlet-name>
  	<url-pattern>/ControllerAjax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControllerFiles</servlet-name>
  	<url-pattern>/ControllerFiles</url-pattern>
  </servlet-mapping>
</web-app>
